<div class="container" id="new">
  <form action="/restaurants" method="POST">
    <div class="mb-3">
      <label for="InputName" class="form-label">name:</label>
      <input type="text" class="form-control" name="name">
    </div>
    <div class="mb-3">
      <label for="exampleInputName_en" class="form-label">name_en:</label>
      <input type="text" class="form-control" name="name_en">
    </div>
    <div class="mb-3">
      <label for="exampleInputCategory" class="form-label">category:</label>
      <input type="text" class="form-control" name="category">
    </div>
    <div class="mb-3">
      <label for="exampleInputImage" class="form-label">image:</label>
      <input type="text" class="form-control" name="image">
    </div>
    <div class="mb-3">
      <label for="exampleInputLocation" class="form-label">location:</label>
      <input type="text" class="form-control" name="location">
    </div>
    <div class="mb-3">
      <label for="exampleInputPhone" class="form-label">phone:</label>
      <input type="text" class="form-control" name="phone">
    </div>
    <div class="mb-3">
      <label for="exampleInputGoogle_map" class="form-label">google_map:</label>
      <input type="text" class="form-control" name="google_map">
    </div>
    <div class="mb-3">
      <label for="exampleInputRating" class="form-label">rating:</label>
      <input type="number" class="form-control" name="rating">
    </div>
    <div class="mb-3">
      <label for="exampleInputDescription" class="form-label">description:</label>
      <input type="text" class="form-control" name="description">
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
  </form>
</div>
